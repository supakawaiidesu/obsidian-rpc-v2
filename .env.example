# Server Configuration
PORT=3000

# RPC URLs (comma-separated)
# Replace with your actual RPC endpoints and API keys
RPC_URLS=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY,https://mainnet.infura.io/v3/YOUR_INFURA_KEY,https://rpc.ankr.com/eth,https://eth.llamarpc.com,https://1rpc.io/eth,https://ethereum.publicnode.com,https://rpc.flashbots.net,https://cloudflare-eth.com

# CORS Configuration
# Comma-separated list of allowed origins (use * for all origins)
CORS_ORIGINS=*

# Request Configuration
# Maximum request size in bytes (default: 1MB)
MAX_REQUEST_SIZE=1048576

# Request timeout in milliseconds (default: 6s)
REQUEST_TIMEOUT=6000

# Maximum concurrent requests per endpoint (default: 200)
MAX_CONCURRENT_REQUESTS=200

# Enable response caching (default: false)
ENABLE_CACHE=false

# Cache TTL in milliseconds (default: 1000ms)
CACHE_TTL=1000